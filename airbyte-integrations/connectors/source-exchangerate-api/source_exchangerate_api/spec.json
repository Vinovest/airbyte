{
  "documentationUrl": "https://www.exchangerate-api.com/docs/overview",
  "connectionSpecification": {
    "title": "Exchangerate Api Spec",
    "type": "object",
    "required": ["api_key", "base_currency", "start_date"],
    "additionalProperties": false,
    "properties": {
        "api_key": {
          "type": "string",
          "description": "API Key for authentication.",
          "pattern": "^[0-9a-z]{24}"
        },
      "base_currency": {
        "type": "string",
        "description": "Base currency for exchange rates.",
        "pattern": "^[A-Z]{3}"
      },
      "start_date": {
        "type": "string",
        "description": "Base currency for exchange rates.",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}"
      }
    }
  }
}
